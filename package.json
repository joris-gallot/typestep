{
  "name": "typestep",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@9.15.5",
  "description": "Simplifies JavaScript to TypeScript migration with incremental changes",
  "author": "Joris Gallot",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joris-gallot/typestep.git"
  },
  "keywords": [
    "tsc"
  ],
  "types": "dist/types.d.ts",
  "bin": {
    "typestep": "dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": "22.15.0"
  },
  "scripts": {
    "playground": "jiti src/playground.ts",
    "cli": "jiti src/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "citty": "^0.1.6",
    "consola": "^3.4.2",
    "jiti": "^2.4.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@types/node": "^20.17.31",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vitest": "^3.1.2"
  }
}
